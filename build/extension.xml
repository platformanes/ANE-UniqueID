        </applicationDeployment>
    </platform>
    <platform name="iPhone-x86">
        <applicationDeployment>
            <nativeLibrary>libUniqueID.a</nativeLibrary>
            <initializer>UniqueIDExtInitializer</initializer>
            <finalizer>UniqueIDExtFinalizer</finalizer>
        </applicationDeployment>
    </platform>

